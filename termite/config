color12=#0094ff

#magenta
color5=#c162c6
color13=#e356eb

#cyan
color14=#00cdcd
colorr=#23e0fc

#white
color7=#e5e5e5
color15=#ffffff

foreground = #dcdccc
#foreground_bold = #ffffff

# 20% background transparency (requires a compositor)
background = rgba(00, 00, 00, 0.85)

# If unset, will reverse foreground and background
highlight = #2f2f2f


[hints]
font = Hack 8
background = #8ec70c
foreground = #282828
